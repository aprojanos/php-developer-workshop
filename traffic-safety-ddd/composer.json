{
  "name": "example/traffic-safety",
  "require": {
    "php": "^8.4",
    "traffic-safety/shared-kernel": "^1.0",
    "vlucas/phpdotenv": "^5.6"
  },
  "repositories": [
    {
      "type": "path",
      "url": "shared-kernel",
      "options": {
        "symlink": true
      }
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
