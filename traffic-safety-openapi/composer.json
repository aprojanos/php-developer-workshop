{
  "name": "example/traffic-safety",
  "require": {
    "php": "^8.4",
    "traffic-safety/shared-kernel": "^1.0",
    "traffic-safety/accident-context": "^1.0",
    "traffic-safety/road-network-context": "^1.0",
    "traffic-safety/countermeasure-context": "^1.0",
    "traffic-safety/hotspot-context": "^1.0",
    "traffic-safety/project-context": "^1.0",
    "traffic-safety/notification-context": "^1.0",
    "traffic-safety/user-context": "^1.0",
    "vlucas/phpdotenv": "^5.6",
    "firebase/php-jwt": "^6.10"
  },
  "repositories": [
    {
      "type": "path",
      "url": "shared-kernel",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "contexts/accident",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "contexts/road-network",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "contexts/countermeasure",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "contexts/hotspot",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "contexts/project",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "contexts/notification",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "contexts/user",
      "options": {
        "symlink": true
      }
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
